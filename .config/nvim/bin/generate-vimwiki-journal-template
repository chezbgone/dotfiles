#!/usr/bin/python
import os.path
import re
import sys

with open('/home/jason/Dropbox/vimwiki/journal/template.wiki') as f:
  template = f.read()

path = sys.argv[1]
file = path.split('/')[-1]
name = file.rsplit('.', maxsplit=1)[0]
if re.match(r'2\d\d\d-\d\d-\d\d', name):
  print(template.format(date=name))
