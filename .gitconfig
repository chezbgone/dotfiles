[alias]
	hist = log --pretty=format:'%C(yellow)%h %C(red)%ad %C(reset)| %s%C(green)%d %C(cyan)[%an]' --decorate --date=short --graph
	undo = reset --soft HEAD~1
	ss = status -s -b
	co = commit

[includeIf "gitdir:~/Dropbox/MIT/6_031/"]
	path = ~/.gitconfig.mit
